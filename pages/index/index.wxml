<view class="page" data-weui-theme="{{theme}}">

<view class="weui-cells weui-cells_after-title">
  <view wx:if="{{goods.length==0}}" class="weui-cell weui-cell_example ">
    <label>暂无数据</label>
  </view>
  <view wx:if="{{goods.length>0}}" wx:for="{{goods}}" class="weui-cell weui-cell_example ">
    <view class="weui-cell__bd">{{item.code}}</view>
    <view class="weui-cell__ft">{{item.value}}</view>
  </view>
</view>

